{
  "name": "react-bknd",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Ondrej Rohon",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cors": "^2.8.1",
    "dotenv-safe": "^4.0.3",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "jwt-decode": "^2.1.0",
    "mongodb": "^2.2.21",
    "multer": "^1.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run cleanDist && npm run compileConfig && NODE_ENV=\"production\" webpack -p --config webpack.config.compiled.js",
    "build:server": "babel server --out-dir .tmpdeploy/server --no-babel-rc --presets react,es2015,stage-2",
    "compileConfig": "babel webpack.config.js -o webpack.config.compiled.js --no-babelrc --presets=es2015,stage-0",
    "cleanDist": "rm -rf dist/assets",
    "dev": "npm run compileConfig && webpack-dev-server --config webpack.config.compiled.js",
    "deploy": "npm run build && bash .deploy/deploy.sh",
    "start": "nodemon server/server.js --exec babel-node --presets react,es2015,stage-2 --watch server",
    "startprod": "NODE_ENV=production node ./server/server.js"
  },
  "devDependencies": {
    "auth0-js": "^7.6.1",
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.1",
    "image-webpack-loader": "^3.2.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "react-bootstrap": "^0.30.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.1",
    "sass-burger": "^1.3.1",
    "sass-loader": "^4.1.1",
    "speakingurl": "^11.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
